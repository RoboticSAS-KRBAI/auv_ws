<launch>
    <group ns='rosserial'>
        <node pkg='rosserial_python' type='serial_node.py' name='node_serial' output='screen'>
            <param name="port" value="/dev/ttyACM0"/>
            <param name="baud" value="57600"/>
        </node>
    </group>

    <group ns='auv'>
        <node pkg='techsas_auv_ros' type='node_boot_time.py' name='node_boot_time' output='screen' />
        <node pkg='techsas_auv_ros' type='node_messages.py' name='node_messages' output='screen' />
        <node pkg='techsas_auv_ros' type='node_navigation.py' name='node_navigation' output='screen' />
        <node pkg='techsas_auv_ros' type='node_guidance.py' name='node_guidance' output='screen' />
        <node pkg='techsas_auv_ros' type='node_control_depth.py' name='node_control_depth' output='screen' />
        <node pkg='techsas_auv_ros' type='node_control_lateral.py' name='node_control_lateral' output='screen' />
        <node pkg='techsas_auv_ros' type='node_control_forward.py' name='node_control_forward' output='screen' />
        <node pkg='techsas_auv_ros' type='node_control_auv.py' name='node_control_auv' output='screen' />
    </group>
    
    <group ns='yolo'>
        <node pkg='yolov5' type='node_object_detection.py' name='node_object_detection' output='screen' />
    </group>
</launch>